{
  "name": "kerbol-launcher",
  "productName": "KerbolLauncher",
  "version": "1.0.0",
  "description": "Unofficial KSP launcher",
  "main": "dist/main.js",
  "scripts": {
    "compile": "tsc && tscpaths -p tsconfig.json -s ./src -o ./dist && copyfiles -u 2 src/frontend/**/* dist/frontend/",
    "start": "yarn compile && electron-forge start",
    "make": "yarn compile && electron-builder",
    "clean": "rimraf dist && rimraf out",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": "Londek",
  "license": "GPL-3.0-only",
  "dependencies": {
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "copyfiles": "^2.4.1",
    "electron": "^12.0.2",
    "electron-builder": "^22.10.5",
    "eslint": "^7.23.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.20.0",
    "rimraf": "^3.0.2",
    "tsconfig-paths": "^3.9.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.0.2",
    "typescript-eslint-parser": "^22.0.0",
    "yarn": "^1.22.10"
  }
}
